{
  "digest": {
    "font-family": "Helvetica, Arial, sans-serif",
    "font-size": "10pt",
    "color": "#000",
    "max-width": "1104px",
    "margin": "0"
  },
  "link": {
    "text-decoration": "none",
    "color": "#4183c4"
  },
  "intro-paragraph": {
    "font-size": "12pt",
    "user-link": {
      "font-weight": "bold",
      "color": "#000"
    },
    "user-avatar": {
      "vertical-align": "bottom",
      "padding-right": "3px"
    }
  },
  "interval-header": {
    "font-size": "24pt",
    "font-weight": "bold",
    "margin": ".75em 0 .5em 0",
    "border-bottom": "solid 1px #ddd"
  },
  "repository-header": {
    "font-size": "18pt",
    "font-weight": "bold",
    "margin": ".5em 0"
  },
  "commit": {
    "background": "#fafafa",
    "border": "solid 1px #ccc",
    "margin": "1em 0",
    "title": {
      "padding": "8px",
      "margin": "0"
    },
    "message": {
      "margin": "0",
      "padding": "0 8px 8px",
      "white-space": "pre-wrap"
    },
    "footer": {
      "background": "#fff",
      "border-top": "solid 1px #ddd",
      "padding": "8px",
      "date": {
        "color": "#666"
      },
      "link": {
        "font-family": "monospace",
        "float": "right"
      }
    }
  }
}
